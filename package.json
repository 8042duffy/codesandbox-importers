{
  "name": "codesandbox-importers",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build":
      "yarn build:dependencies && lerna run build --scope @codesandbox/cli --scope git-converter --stream",
    "build:dependencies":
      "lerna run build --scope @codesandbox/import-utils --scope @codesandbox/types --stream",
    "build:git-extractor":
      "yarn build:dependencies && lerna run build --scope git-converter",
    "test": "lerna run test",
    "start": "lerna run start --stream"
  },
  "author": "Ives van Hoorne",
  "devDependencies": {
    "lerna": "^2.4.0"
  },
  "workspaces": [
    "packages/cli",
    "packages/git-extractor",
    "packages/import-utils",
    "packages/types"
  ]
}
